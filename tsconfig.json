{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es7"],
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["dist", "es", "lib", "cases"],
  "typings": "./typings.d.ts"
}
